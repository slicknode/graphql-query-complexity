{
  "name": "graphql-query-complexity",
  "version": "0.9.0",
  "description": "Validation rule for GraphQL query complexity analysis",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "module": "dist/esm/index.js",
  "scripts": {
    "lint": "eslint --ext .ts . && prettier --config .prettierrc 'src/**/*.ts' --check",
    "lint:fix": "eslint --ext .ts . --fix && prettier --config .prettierrc 'src/**/*.ts' --write",
    "clean": "rimraf dist/*",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "build:cjs": "tsc -p ./tsconfig.json",
    "test": "npm run lint && npm run testonly",
    "testonly": "mocha --check-leaks --exit --full-trace --require ts-node/register/transpile-only 'src/**/__tests__/**/*-test.{ts,tsx}'",
    "dist": "npm run clean && tsc && npm run build",
    "prepare": "npm run clean && npm run dist"
  },
  "directories": {
    "lib": "./dist"
  },
  "dependencies": {
    "lodash.get": "^4.4.2"
  },
  "peerDependencies": {
    "graphql": "^14.5.0 || ^15.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": "slicknode/graphql-query-complexity",
  "keywords": [
    "graphql",
    "query",
    "validation",
    "cost",
    "complexity",
    "analysis"
  ],
  "author": "Ivo Mei√üner",
  "license": "MIT",
  "devDependencies": {
    "@types/assert": "^1.4.6",
    "@types/chai": "^4.2.11",
    "@types/lodash.get": "^4.4.6",
    "@types/mocha": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "graphql": "^14.5.0 || ^15.0.0",
    "mocha": "^7.1.1",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  }
}
